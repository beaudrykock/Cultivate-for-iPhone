			<string>We will look after your details and not pass them to anyone else</string>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSTextFieldSpecifier</string>
			<key>Title</key>
			<string>Name</string>
			<key>Key</key>
			<string>pref_name</string>
			<key>DefaultValue</key>
			<string>(required)</string>
			<key>KeyboardType</key>
			<string>Alphabet</string>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSTextFieldSpecifier</string>
			<key>Title</key>
			<string>Mobile</string>
			<key>DefaultValue</key>
			<string>(required)</string>
			<key>Key</key>
			<string>pref_mobile</string>
