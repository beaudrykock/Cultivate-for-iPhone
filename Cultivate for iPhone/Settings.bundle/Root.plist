<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PreferenceSpecifiers</key>
	<array>
		<dict>
			<key>Title</key>
			<string>Reminders</string>
			<key>Type</key>
			<string>PSGroupSpecifier</string>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSToggleSwitchSpecifier</string>
			<key>Title</key>
			<string>Show reminders</string>
			<key>Key</key>
			<string>pref_showReminders</string>
			<key>DefaultValue</key>
			<true/>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSMultiValueSpecifier</string>
			<key>Title</key>
			<string>Time before</string>
			<key>Key</key>
			<string>pref_timeBefore</string>
			<key>DefaultValue</key>
			<integer>600</integer>
			<key>Values</key>
			<array>
				<integer>600</integer>
				<integer>1800</integer>
				<integer>3600</integer>
				<integer>86400</integer>
			</array>
			<key>Titles</key>
			<array>
				<string>10 minutes</string>
				<string>30 minutes</string>
				<string>1 hour</string>
				<string>1 day</string>
			</array>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSMultiValueSpecifier</string>
			<key>Title</key>
			<string>Repeat</string>
			<key>Key</key>
			<string>pref_repeatPattern</string>
			<key>DefaultValue</key>
			<integer>0</integer>
			<key>Values</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
				<integer>2</integer>
			</array>
			<key>Titles</key>
			<array>
				<string>One-off</string>
				<string>Weekly</string>
				<string>Monthly</string>
			</array>
		</dict>
		<dict>
			<key>Type</key>
			<string>PSToggleSwitchSpecifier</string>
			<key>Title</key>
			<string>Apply to all reminders</string>
			<key>Key</key>
			<string>pref_applySettingsToAllReminders</string>
			<key>DefaultValue</key>
			<false/>
		</dict>
		<dict>
			<key>Title</key>
			<string>CultiRide user details</string>
			<key>Type</key>
			<string>PSGroupSpecifier</string>
